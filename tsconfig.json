{
  "compilerOptions": {
    "target": "es2020",

    "module": "ESNext",
    "strict": true,
    "moduleResolution": "node",
    "types": [
    ],
    "resolveJsonModule": true,
    "noEmit": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
}
